{
  "name": "pg_to_pgstream",
  "wire_format": "json",
  "source": {
    "name": "pg-source",
    "type": "postgres",
    "options": {
      "dsn": "postgres://user:pass@localhost:5432/app?sslmode=disable",
      "slot": "wallaby_slot",
      "publication": "wallaby_pub",
      "batch_size": "500",
      "batch_timeout": "1s",
      "status_interval": "10s",
      "create_slot": "true",
      "format": "json"
    }
  },
  "destinations": [
    {
      "name": "stream-out",
      "type": "pgstream",
      "options": {
        "dsn": "postgres://user:pass@localhost:5432/wallaby?sslmode=disable",
        "stream": "orders",
        "format": "json"
      }
    }
  ]
}
