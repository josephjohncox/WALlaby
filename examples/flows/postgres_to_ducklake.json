{
  "name": "pg_to_ducklake",
  "wire_format": "json",
  "source": {
    "name": "pg-source",
    "type": "postgres",
    "options": {
      "dsn": "postgres://user:pass@localhost:5432/app?sslmode=disable",
      "slot": "wallaby_slot",
      "publication": "wallaby_pub",
      "batch_size": "1000",
      "batch_timeout": "2s",
      "status_interval": "10s",
      "create_slot": "true",
      "format": "json"
    }
  },
  "destinations": [
    {
      "name": "ducklake-out",
      "type": "ducklake",
      "options": {
        "dsn": "duckdb.db",
        "catalog": "metadata.ducklake",
        "catalog_name": "lake",
        "data_path": "s3://my-ducklake-data",
        "override_data_path": "true"
      }
    }
  ]
}
